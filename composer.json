{
    "name": "abdallahmohammed/alsaad-php",
    "description": "PHP Client for using Alsaad's API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AbdallahMohammed",
            "email": "abdallah.r660@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "zendframework/zend-diactoros": "^2.2",
        "php-http/guzzle6-adapter": "^1.0",
        "ocramius/package-versions": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "php-http/mock-client": "^0.3.0",
        "estahn/phpunit-json-assertions": "^3.0.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Alsaad\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alsaad\\": "test/",
            "AlsaadTest\\": "test/"
        }
    }
}
